{
    "name": "lexik/fixtures-mapper-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provide some classes to easily load fixtures from CSV or Yaml files.",
    "keywords": ["Symfony2", "bundle", "fixtures"],
    "homepage": "https://github.com/lexik/LexikFixturesMapperBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Barthe",
            "email": "j.barthe@lexik.fr"
        },
        {
            "name": "Dev Lexik",
            "email": "dev@lexik.fr"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.5"
    },
    "require-dev": {
      "phpunit/phpunit": "3.7.*",
      "doctrine/data-fixtures": "*",
      "doctrine/orm": "*"

    },
    "autoload":     {
        "psr-0": { "Lexik\\Bundle\\FixturesMapperBundle": "" }
    },
    "target-dir": "Lexik/Bundle/FixturesMapperBundle"
}
